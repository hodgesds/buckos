# Buck2 configuration for sideros
# Compatible with sideros-build buck definitions

[cells]
root = .
prelude = prelude
toolchains = toolchains
third-party = third-party

[cell_aliases]
config = prelude
ovr_config = prelude
fbcode = root
fbsource = root
buck = root

[external_cells]
prelude = bundled

[build]
execution_platforms = root//platforms:default

[rust]
default_edition = 2021
